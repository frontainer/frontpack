  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "webpack",
    "serve": "webpack -w",
    "serve:prod": "npm run aot && NODE_ENV='production' webpack-dev-server -p",
    "production": "npm run aot && NODE_ENV='production' webpack -p",
    "test": "karma start",
    "dll": "webpack --config webpack.dll.config.js",
    "map": "source-map-explorer public/assets/js/main.js public/assets/js/main.js.map",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor -p protractor.conf.js",
    "aot": "ngc -p tsconfig.json",
    "lint": "tslint src/assets/js/**/*.ts && stylelint 'src/assets/css/**/*s?(c|a)ss'"
  },
