    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "2.0.1",
    "protractor": "4.0.14",
    "source-map-explorer": "1.3.3",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "ts-node": "2.0.0",
